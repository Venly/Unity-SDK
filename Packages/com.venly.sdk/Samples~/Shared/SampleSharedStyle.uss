}

.unity-text-field__label > mainlabelfield_text {
}

.unity-base-field > mainlabelfield_base {
}

.button-select {
    height: 30px;
    min-width: 20px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 5px;
    padding-bottom: 5px;
    font-size: 16px;
    border-top-left-radius: 15px;
    border-bottom-left-radius: 15px;
    border-top-right-radius: 15px;
    border-bottom-right-radius: 15px;
}

.main-labelfield-details {
    margin-bottom: -7px;
}
